{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "products",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "current_price": {
      "type": "decimal"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "published": {
      "type": "date"
    },
    "delivery": {
      "type": "boolean"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "books",
        "films",
        "music",
        "electronics",
        "computers",
        "home",
        "garden",
        "pets",
        "toys",
        "children",
        "baby",
        "clothes",
        "shoes",
        "jewellery",
        "accessories"
      ]
    },
    "questions": {
      "collection": "questions",
      "via": "product"
    },
    "about": {
      "type": "json"
    },
    "rrp": {
      "type": "decimal"
    },
    "rating": {
      "type": "integer"
    },
    "details": {
      "type": "json"
    }
  }
}
